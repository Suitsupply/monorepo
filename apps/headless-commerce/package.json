{
  "name": "@susu/headless-commerce",
  "version": "0.1.0",
  "description": "Suitsupply headless commerce",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "codegen": "graphql-codegen --config codegen.ts",
    "dev": "next dev",
    "dev-only": "next dev",
    "install:playwright": "pnpm exec playwright install",
    "lint": "pnpm lint:tsc && pnpm lint:eslint && pnpm lint:style",
    "lint:eslint": "eslint --fix --cache --cache-location .eslintcache --no-error-on-unmatched-pattern .",
    "lint:style": "stylelint --allow-empty-input '**/*.{css,scss}'",
    "lint:tsc": "tsc -p . --noEmit",
    "start": "next start",
    "test": "vitest run --passWithNoTests",
    "test-coverage": "vitest run --coverage",
    "test:e2e": "playwright test --fully-parallel",
    "test:e2e:ui": "playwright test --fully-parallel --ui",
    "update-packages": "pnpx npm-check-updates -u && pnpm i",
    "vercel:link": "pnpm run vercel:link:testing",
    "vercel:link:acceptance": "pnpm vercel link --yes --cwd . --scope suitsupply-team --project susu-monorepo_headless-commerce_acceptance",
    "vercel:link:testing": "pnpm vercel link --yes --cwd . --scope suitsupply-team --project susu-monorepo_headless-commerce_testing"
  },
  "dependencies": {
    "@ariakit/react": "^0.4.7",
    "@contentful/rich-text-react-renderer": "^15.21.2",
    "@datadog/browser-rum": "^5.20.0",
    "@dev.mohe/isomorphic-webcrypto": "^1.0.1",
    "@preact/signals-react": "^2.0.1",
    "@segment/analytics-next": "^1.70.0",
    "@suits/crm-booking-tool": "0.0.96",
    "@suits/meas-size-chart": "1.6.2",
    "@suits/ss-design-system": "^7.34.7",
    "@susu/providing-application": "workspace:*",
    "@typescript-eslint/parser": "^7.12.0",
    "@urql/core": "^5.0.4",
    "@urql/exchange-auth": "^2.2.0",
    "@urql/exchange-retry": "^1.3.0",
    "@urql/next": "^1.1.1",
    "akamai-edgegrid": "^3.4.5",
    "avo-inspector": "^1.3.0",
    "classnames": "^2.5.1",
    "commerce-sdk-isomorphic": "^2.0.0",
    "deepmerge": "4.3.1",
    "embla-carousel-react": "8.1.3",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "next": "14.2.3",
    "next-intl": "3.15.0",
    "pino": "^9.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-gtm-module": "^2.0.11",
    "responsive-image-utils": "^1.1.1",
    "rotating-file-stream": "^3.2.1",
    "tslog": "^4.9.2",
    "type-fest": "^4.20.0",
    "urql": "^4.1.0",
    "uuid": "^9.0.1",
    "wonka": "^6.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@contentful/rich-text-types": "^16.5.2",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/fragment-matcher": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/schema-ast": "^4.0.2",
    "@graphql-codegen/typescript-document-nodes": "^4.0.7",
    "@graphql-codegen/typescript-operations": "^4.2.1",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@playwright/test": "^1.44.1",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/react": "^16.0.0",
    "@types/gtag.js": "^0.0.20",
    "@types/jsdom": "^21.1.7",
    "@types/node": "20.14.2",
    "@types/react-dom": "18.3.0",
    "@types/react-gtm-module": "^2.0.3",
    "@types/uuid": "^9.0.8",
    "@vercel/edge-config": "^1.1.1",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "dotenv": "^16.4.5",
    "jose": "^5.4.0",
    "jsdom": "^24.1.0",
    "sass": "^1.77.4",
    "stylelint": "^16.6.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "5.4.5",
    "vercel": "^34.2.5",
    "vitest": "^1.6.0"
  }
}
